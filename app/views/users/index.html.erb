<!-- User Index -->

<div class="jumbotron jumbotron-fluid text-center bg-info" style = "width: 100%; background: rgb(63,94,251);
background: radial-gradient(circle, rgba(63,94,251,1) 0%, rgba(146,70,252,1) 100%); ">
	<div class="container">
		<h1>See What Others Want To Watch!</h1>
	</div>
</div>


  <hr class="mt-2 mb-5">

  <div class="row text-center text-lg-left">


<% @users.each do |user| %>
    <div class="col-lg-3 col-md-4 col-6">
    <div class="box text-center">
        <a href="#" class="d-block mb-4 h-100">
            <img class="img-fluid img-thumbnail" src="data:image/png;base64," alt="">
        </a>
        <div class="text-center">
            <%= link_to user.name, user %>
			<%user.list_watchlist.each do |watchlist|%>
				<p>-<%= watchlist %></p>
			<% end %>
			<br>
        </div>
    </div>

    </div>
<% end %>
  </div>